<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.jojinwoo.learn2caltest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:label="@string/app_name"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:replace="android:icon,android:theme"
        >
        <activity android:name=".Activity.MainActivity" android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.DemographicActivity" android:label="Demographic" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden|screenSize"  />
        <activity android:name=".Activity.QuizActivity" android:label="Quiz" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity android:name=".Activity.FinalActivity" android:label="Final" android:screenOrientation="portrait" android:configChanges="orientation|keyboardHidden|screenSize" />

    </application>

</manifest>